.profileContentArea.listingPage
  .profilePageHeader
    %h2.columnDescriptor
      %span
        Editing
      V4Asset
  .listings

    .box

      - form_for(@v4_asset) do |f|
        = f.error_messages

        = render :partial =>  'form', :locals => { :f => f }

        %p
          = f.submit "Update"

      = link_to 'Show', @v4_asset
      |
      = link_to 'Back', v4_assets_path
